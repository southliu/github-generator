{
  "name": "github-generator",
  "version": "0.0.1",
  "description": "使用一串代码自动生成github page所需的静态文件",
  "main": "./main.ts",
  "bin": {
    "github": "./bin/main.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc && copyfiles ./templates/* ./bin",
    "adduser": "npm adduser",
    "first-npm": "npm publish --access public",
    "upload": "tsc && copyfiles ./templates/* ./bin && concurrently \"npm publish\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SouthLiu/github-generator.git"
  },
  "keywords": [
    "github-generator",
    "github",
    "blog",
    "generator",
    "自动生成",
    "博客"
  ],
  "files": [
    "bin"
  ],
  "author": {
    "name": "South",
    "email": "1275093225@qq.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SouthLiu/github-generator/issues"
  },
  "homepage": "https://github.com/SouthLiu/github-generator#readme",
  "dependencies": {
    "commander": "^8.3.0",
    "download-git-repo": "^3.0.2",
    "ejs": "^3.1.6",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "loading-cli": "^1.1.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.5",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "typescript": "^4.5.4"
  }
}
